%h2 Edit Document
%table#documents_table
  %thead
    %tr
      %th.id Key
      %th.value Value

  %tbody
  - @document.each do |key, value|
    %tr
      %td
        %code{:class=> "key"}= key
      %td
        - if value.is_a? Hash
          = partial 'collection/document', :object => value
        - else
          %code{:class=> determin_style_class_for(value) }= value